<div class="b-header">
    <h1>Read more about GPV
    </h1>
  </div>
  <div class="b-container">
    <% blogPosts.forEach(post => { %>
        <div class="b-blog">
            <div class="cont">
                <h2><%= post.title %></h2>
                <p class="time"><%= post.submissionTime.toDateString() %></p>
                <p class="aciklama"><%= post.shortDescription %></p>
                <div class="bosluk"></div>
            </div>
        </div>
    <% }); %>
</div>